<p id="notice"><%= notice %></p>

<h1>Shippings</h1>

<table>
  <thead>
    <tr>
      <th>Date shipped</th>
      <th>Customer</th>
      <th>Address</th>
      <th>Order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shippings.each do |shipping| %>
      <tr>
        <td><%= shipping.date_shipped %></td>
        <td><%= shipping.customer %></td>
        <td><%= shipping.address %></td>
        <td><%= shipping.order %></td>
        <td><%= link_to 'Show', shipping %></td>
        <td><%= link_to 'Edit', edit_shipping_path(shipping) %></td>
        <td><%= link_to 'Destroy', shipping, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shipping', new_shipping_path %>
