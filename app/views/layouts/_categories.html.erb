<h1>Categories</h1>
<br>

  <% @categories.each do |category| %>
       <div class="itemhols">
           <p><%= category.title %></p>
           <%= link_to (image_tag category.image, :class=>"itemholsIm"), category %>
           <p><%= category.description %></p>
         </div>
    
    <% end %>

