<div class="brandSuccess">
   <div class="w3-card-4">
      <div class="w3-container w3-deep-orange w3-text-white w3-center w3-wide">
        <h2>Brand Details</h2>
      </div>

      <p id="notice"><%= notice %></p>
      
    <div class="w3-center w3-wide">
      <p> 
        <strong>Brand name:</strong>
        <%= @brand.brand_name %>
      </p>
      
      <p>
        <strong>Description:</strong>
        <%= @brand.description %>
      </p>
      
      <p>
        <!--<strong>Yearofestablishement:</strong>-->
        <strong>Established:</strong>
        <%= @brand.yearofestablishement %>
      </p>
      
      <div class="w3-panel w3-pale-green w3-padding-24">
        <%= link_to 'Edit', edit_brand_path(@brand), :class => "w3-button w3-orange w3-hover-green w3-round-large w3-margin-left", :role => "button" %> 
        <%= link_to 'Back', brands_path, :class => "w3-button w3-orange w3-hover-green w3-round-large w3-margin-left", :role => "button" %>
      </div>
      
    </div>  
      
    </div>
  </div>
